---
import { Image } from '@astrojs/image/components'
import Layout from '../layouts/Layout.astro'
import { getCollection } from 'astro:content'
import logo from '../images/logo.png'
import PinecastPlayer from '../components/PinecastPlayer.astro'
import PinkBlob from '../components/PinkBlob.astro'
import BlueBlob from '../components/BlueBlob.astro'
import YellowBlob from '../components/YellowBlob.astro'
import SubmitJokeForm from '../components/SubmitJokeForm.astro'

const episodes = await getCollection('episodes')

const sortedEpisodes = episodes
  .sort((a, b) => b.data.date.getTime() - a.data.date.getTime())
  .slice(0, 16)
const [latestEpisode, ...restEpisodes] = sortedEpisodes
const { Content: LatestEpisodeContent } = await latestEpisode.render()
